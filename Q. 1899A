#include <cctype>
#include <iostream>
#include <type_traits>
#include <vector>
#include <string>
#include <algorithm>
#include <stack>
#include <queue>
#include <deque>
#include <bits/stdc++.h>

using namespace std;

typedef long long ll;

int main() {

    int tt;
    cin>>tt;
    while(tt--) {

        int n;
        cin>>n;

            if((n + 1) % 3 == 0 || (n - 1) % 3 == 0) {
                cout<<"First"<<endl;
            }
            else {
                cout<<"Second"<<endl;
            }
    }
}
