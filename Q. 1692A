#include <cctype>
#include <iostream>
#include <type_traits>
#include <vector>
#include <string>
#include <algorithm>
#include <stack>
#include <queue>
#include <deque>
#include <bits/stdc++.h>

using namespace std;

typedef long long ll;

int main() {

    int tt;
    cin>>tt;
    while(tt--) {

        int arr[4];
        for(int i=0;i<4;i++) {
            cin>>arr[i];
        }

        int count = 0;

        for(int i=1;i<4;i++) {
            if(arr[0] < arr[i]) {
                count++;
            }
        }

        cout<<count<<endl;
    }
}
