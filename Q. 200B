#include <iostream>
#include <vector>
#include <string>
#include <algorithm>
#include <iomanip>

using namespace std;

int main()
{
    int n;
    cin>>n;

    int arr[n]; 
    for(int i=0;i<n;i++) {
        cin>>arr[i];
    }

    int sum = 0;
    for(int i=0;i<n;i++) {
        sum += arr[i];
    }

    double ans = static_cast<double> (sum)/ n;
    cout<<ans<<endl;
    return 0;
}
