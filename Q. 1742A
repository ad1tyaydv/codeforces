#include <cctype>
#include <iostream>
#include <type_traits>
#include <vector>
#include <string>
#include <algorithm>
#include <stack>
#include <queue>
#include <deque>
#include <bits/stdc++.h>

using namespace std;

typedef long long ll;

int main() {

    int tt;
    cin>>tt;
    while(tt--) {

        int a,b,c;
        cin>>a>>b>>c;

        if(a + b == c || a + c == b || b + c == a)  {
            cout<<"YES"<<endl;
        }
        else {
            cout<<"NO"<<endl;
        }
    }
}
