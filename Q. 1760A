#include <cctype>
#include <iostream>
#include <type_traits>
#include <vector>
#include <string>
#include <algorithm>
#include <stack>
#include <queue>
#include <deque>
#include <bits/stdc++.h>

using namespace std;

typedef long long ll;

int main() {
    
    int tt;
    cin>>tt;
    while(tt--) {

        int a,b,c;
        cin>>a>>b>>c;

        int middle = a + b + c - min({a, b, c}) - max({a, b, c});

        cout<<middle<<endl;
    }
}
