#include <cctype>
#include <iostream>
#include <type_traits>
#include <vector>
#include <string>
#include <algorithm>
#include <stack>
#include <queue>
#include <deque>
#include <bits/stdc++.h>

using namespace std;

typedef long long ll;

int main() {

    int tt;
    cin>>tt;

    while(tt--) {

        string s;
        cin>>s;

        transform(s.begin(), s.end(), s.begin(), ::tolower);

        if(s == "yes") {
            cout<<"YES"<<endl;
        }
        else {
            cout<<"NO"<<endl;
        }
    }
}
