    using namespace std;
     
    typedef long long ll;
     
    int main() {
     
        int a,b;
        cin>>a>>b;
     
        int mini = min(a,b);
        int maxi = max(a,b);
     
        cout<<mini<<" ";
     
        int left = maxi - mini;
        cout<<(left / 2)<<endl;
        
    }
