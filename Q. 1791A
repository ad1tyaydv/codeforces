#include <cctype>
#include <iostream>
#include <type_traits>
#include <vector>
#include <string>
#include <algorithm>
#include <stack>
#include <queue>
#include <deque>
#include <bits/stdc++.h>

using namespace std;

typedef long long ll;

int main() {
    
    int tt;
    cin>>tt;
    while(tt--) {

        string s = "codeforces";
        char ch;
        cin>>ch;
        
        bool flag = false;
        for(int i=0;i<10;i++) {
            if(ch == s[i]) {
                flag = true;
                break;
            }
        }
        

        if(flag) {
            cout<<"YES"<<endl;
        }
        else {
            cout<<"NO"<<endl;
        }
    }
}
